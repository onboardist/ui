@import url('https://fonts.googleapis.com/css?family=Short+Stack');
  
.theme-container.myhome .home .hero img {
  max-height: 10vh;
}

.example {
  text-align: center;
  border: 3px dashed lightgray;
  border-radius: 8px;
  padding: 40px;

  button {
    background: $buttonColor;
    color: #fff;
    font-size 1.66rem;
    border: none;
    border-radius: 4px;
    padding: 10px 20px;
    text-transform: uppercase;
    outline: none;
    cursor: pointer;
  }
}